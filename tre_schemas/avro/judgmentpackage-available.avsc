{
  "type": "record",
  "name": "JudgmentPackageAvailable",
  "namespace": "uk.gov.nationalarchives.tre.messages.judgmentpackage.available",
  "fields": [
    {
      "name": "properties",
      "type": "uk.gov.nationalarchives.tre.messages.event.Properties"
    },
    {
      "name": "parameters",
      "type": {
        "type": "record",
        "name": "Parameters",
        "fields": [
          {
            "name": "reference",
            "type": "string",
            "doc": "(TDR-2023-AAA|FCL-???????) # opaque string: use originator to know where it came from, should be an acceptable filename"
          },
          {
            "name": "originator",
            "type": [
              "string",
              "null"
            ],
            "doc": "who provided the docx such as FCL|TDR. This may have been passed from the judgmentparse"
          },
          {
            "name": "bundleFileURI",
            "type": "string",
            "doc": "download tar.gz"
          },
          {
            "name": "metadataFilePath",
            "type": "string",
            "doc": "metadata file location in bundle tar.gz ",
            "default": "metadata.json"
          },
          {
            "name": "metadataFileType",
            "type": "string",
            "doc": "type definition of the metadata file defined by a schema"
          },
          {
            "name": "status",
            "type": {
              "type": "enum",
              "name": "Status",
              "symbols": [
                "JUDGMENT_PARSE_NO_ERRORS",
                "JUDGMENT_PARSE_WITH_ERRORS"
              ]
            },
            "doc": "The status as emitted by the parser. Could be used for filtering and redirecting messages"
          }
        ]
      }
    }
  ]
}
